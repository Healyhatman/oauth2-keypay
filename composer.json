{
    "name": "healyhatman/oauth2-keypay",
    "description": "Provider for the OAuth 2 client",
    "keywords": [
        "healyhatman",
        "keypay"
    ],
    "homepage": "https://github.com/healyhatman/oauuth2-keypay",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamron Brooks",
            "email": "kamron@skyspider.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Healyhatman\\Skeleton\\": "src",
            "Healyhatman\\Skeleton\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
